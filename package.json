{
  "name": "pagic-template",
  "version": "0.0.1",
  "description": "这是一个Pagic示例项目",
  "main": "README.md",
  "scripts": {
    "init-pagic": "deno install -f --unstable --allow-read --allow-write --allow-net --allow-run --name=pagic https://deno.land/x/pagic/mod.ts",
    "start": "pagic build --serve --watch"
  },
